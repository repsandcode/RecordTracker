      <div class="group relative">
        <div class="relative group-hover:cursor-crosshair group-hover:opacity-80 aspect-w-1 aspect-h-1 w-full rounded-lg px-4 py-2.5 overflow-hidden bg-base-100 shadow-xl">       
          <!-- can add google maps location at here above description -->
          <div class="">
            <!-- name and button -->
            <div class="mb-4">
              <h1 class="text-gray-900 font-medium text-xl md:text-2xl">Dhananjay Mandal</h1>
              <!-- possible button to update record -->
            </div>
            <!-- details of the householder -->
            <div class="flex justify-between mb-4">
              <!-- left side -->
              <div class="border-solid">
                <span class="block flex items-center mb-0.5 text-sm md:text-base font-medium">
                  <span class="w-5">
                    <i class="fa-solid fa-language"></i>
                  </span>
                  <span class="ml-3">Nepali</span>
                </span>
                <span class="block flex items-center mb-0.5 text-sm md:text-base font-medium">
                  <span class="w-5 flex justify-center">
                    <i class="fa-solid fa-location-dot"></i>
                  </span>
                  <span class="ml-3">Amado</span>
                </span>
              </div>
              <!-- right side -->
              <div class="border-solid pl-8">
                <ul class="outline-0 flex flex-wrap flex-row-reverse">
                  <li class="flex items-center justify-center rounded-full border border-indigo-100 bg-black px-3 py-1.5 ml-0.5 mb-0.5 text-xs md:text-sm text-white font-normal">Bible Study</li>
                  <li class="flex items-center justify-center rounded-full border border-indigo-100 bg-black px-3 py-1.5 ml-0.5 mb-0.5 text-xs md:text-sm text-white font-normal">English Dependent</li>
                  <li class="flex items-center justify-center rounded-full border border-indigo-100 bg-black px-3 py-1.5 ml-0.5 mb-0.5 text-xs md:text-sm text-white font-normal">Attending Meetings</li>
                </ul>
              </div>
            </div>
            <!-- last updated -->
