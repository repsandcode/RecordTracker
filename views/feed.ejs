<%- include('partials/header') -%>


<div class="bg-indigo-50">
  <div class="mx-auto max-w-6xl px-6 lg:px-8 pt-12 pb-14 sm:py-12 md:py-10 lg:py-8 flex items-center justify-between">
    <div class="font-semibold">
      <span class="block text-sm md:text-lg lg:text-xl ">Hi <%= user.firstName %>,</span>
      <span class="block text-2xl md:text-3xl lg:text-4xl">Good Morning!</span>
    </div>
    <div class="flex">
      <div class="md:ml-3 inline-flex rounded-md shadow">
        <a href="/profile" class="flex items-center justify-center rounded-lg border border-indigo-100 bg-white px-3 py-2 text-xs sm:text-sm md:text-base font-medium text-indigo-600 hover:bg-indigo-50">Back to Profile</a>
      </div>
    </div>
  </div>

  <!-- start of feed -->
  <div class="mx-auto max-w-6xl px-6 lg:px-8 py-8">
